<idea-plugin version="2">
  <id>foo.initial</id>
  <name>StaticAnalysis</name>
  <version>1.0</version>

  <description><![CDATA[
      Provides static analysis for the XML DSL provided by Apache Camel.<br>
      ]]></description>

  <change-notes><![CDATA[
      Initial investigation.
      ]]>
  </change-notes>


  <idea-version since-build="107.105"/>

  <application-components>

  </application-components>

  <project-components>

  </project-components>

  <actions>
      <group id="StaticAnalysis" text="StaticAnalysis"
             description="StaticAnalysis tooling for XML Based DSLs">
        <add-to-group group-id="MainMenu" anchor="last" />
          <action id="StaticAnalysis.GenerateGraph" class="foo.GenerateGraphAction" text="Generate Graph" description="Generates a Graph from the Xml Dsl" />
      </group>
  </actions>

  <extensions defaultExtensionNs="com.intellij">
      <dom.fileDescription id="BlueprintDom" implementation="foo.BlueprintDom"/>
      <fileEditorProvider implementation="foo.EditorTest" />

<!--    <toolWindow id="Hello world?" icon="/Camel_16x16.png" anchor="right" factoryClass="foo.GraphWindowFactory" />-->
  </extensions>
</idea-plugin>